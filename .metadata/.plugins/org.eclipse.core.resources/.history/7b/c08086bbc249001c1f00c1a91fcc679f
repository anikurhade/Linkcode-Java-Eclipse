
public class AreaThread extends Thread {
int a,b,add;

public int getA() {
	return a;
}

public void setA(int a) {
	this.a = a;
}

public int getB() {
	return b;
}

public void setB(int b) {
	this.b = b;
}

public int getAdd() {
	return add;
}

public void setAdd(int add) {
	this.add = add;
}

public AreaThread(int a, int b, int add) {
	super();
	this.a = a;
	this.b = b;
	this.add = add;
}

}
